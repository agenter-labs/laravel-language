{
    "name": "agenter-labs/laravel-language",
    "description": "Language switcher package for Laravel",
    "keywords": [
        "laravel",
        "language",
        "switcher"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulla",
            "email": "dev@agenter.com",
            "homepage": "https://agenter.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.5",
        "jenssegers/agent": "2.6.*"
    },
    "autoload": {
        "psr-4": {
            "Akaunting\\Language\\": "./src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akaunting\\Language\\Provider"
            ],
            "aliases": {
                "Language": "Akaunting\\Language\\Facade"
            }
        }
    }
}
